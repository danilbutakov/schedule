{
   "appId": "com.thirdTeam.schedule",
   "appName": "schedule",
   "webDir": "build",
   "bundledWebRuntime": "false",
   "plugins": {
      "GoogleAuth": {
         "scopes": ["profile", "email"],
         "serverClientId": "676713652988-d3r9vlsdbgj2oocs6q9munnc0jjkaovk.apps.googleusercontent.com",
         "forceCodeForRefreshToken": "true"
      },
      "CapacitorFirebaseAuth": {
         "providers": ["google.com", "phone"],
         "languageCode": "en",
         "nativeAuth": true,
         "properties": {
            "google": {
               "hostedDomain": "https://thirdteam-schedule.online"
            }
         },   
         "permissions": {
            "google": ["profile", "https://www.googleapis.com/auth/drive"]
         }
      }
   },
   "android": {
      "path": "android"
   },
   "ios": {
      "path": "ios"
   },
   "server": {
      "url": "https://thirdteam-schedule.online"
   }
}
